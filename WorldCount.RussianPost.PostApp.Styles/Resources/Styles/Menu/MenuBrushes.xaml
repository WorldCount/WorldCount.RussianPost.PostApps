<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:p="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">

    <SolidColorBrush x:Key="MenuBrush:ItemBorderBrush" Color="#cccccc" p:Freeze="true"/>

    <SolidColorBrush x:Key="MenuBrush:BorderBrush" Color="#bebebe" p:Freeze="true"/>
    <SolidColorBrush x:Key="MenuBrush:ForegroundBrush" Color="#262626" p:Freeze="true"/>
    <SolidColorBrush x:Key="MenuBrush:BackgroundBrush" Color="#f0f0f0" p:Freeze="true"/>

    <SolidColorBrush x:Key="MenuBrush:HighBorderBrush" Color="#272b30" p:Freeze="true"/>
    <SolidColorBrush x:Key="MenuBrush:HighBackgroundBrush" Color="#272b30" p:Freeze="true"/>
    <SolidColorBrush x:Key="MenuBrush:HighForegroundBrush" Color="#f0f0f0" p:Freeze="true"/>

    <SolidColorBrush x:Key="MenuBrush:DisableForegroundBrush" Color="#FF707070" p:Freeze="true"/>
    <SolidColorBrush x:Key="MenuBrush:DisableFillBrush" Color="#FF707070" p:Freeze="true"/>

    <SolidColorBrush x:Key="MenuBrush:FillBrush" Color="#f0f0f0" p:Freeze="true"/>
    <SolidColorBrush x:Key="MenuBrush:SubMenuBrush" Color="#bebebe" p:Freeze="true"/>
    <SolidColorBrush x:Key="MenuBrush:SubMenuBorderBrush" Color="#bebebe" p:Freeze="true"/>
    
</ResourceDictionary>